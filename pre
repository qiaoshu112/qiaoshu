    $(document).ready(function(){

        //$('div.contentContainer').hide();

        $('#familyForm').change(function(){ //use change event to register any time a form element changes

            $('div.clanContainer').hide(); //hide all divs before selecting which one to show

            var selectedClan = $(this).val(); //retrieves value attribute of selected option

            $('div#' + selectedClan + '_clan').show();

            $('div#familyTreeContainer div.photoCredit').html('<p>PHOTOS ' + photoCredits[selectedClan] + '</p>');

            if (selectedClan == 'wang') {

                $('#familyTreeBackground').hide();

                $('#familyTreeBackground2').show();

            }

            else if (selectedClan == 'deng') {

                $('#familyTreeBackground').hide();

                $('#familyTreeBackground2').show();

            }

            else {

                $('#familyTreeBackground2').hide();

                $('#familyTreeBackground').show();          

            }

        });

        

        $('#connectNav').click(viewConnections);

        $('#patNav').click(viewPatriarchs);

        $('#familyTreeNav').click(viewTrees);

        $('#trendsNav').click(viewTrends);

        

        $('.backToPats').click(viewPatriarchs);

        $('.backToPats').on("mouseenter", function(){

            $(this).css("color","#414042");

            $('.backArrow').css("background-image","http://go.bloomberg.com/multimedia/files/2012/12/arrowBack_dark.png");

        });

        $('.backToPats').on("mouseleave", function(){

            $(this).css("color","#808285");

            $('.backArrow').css("background-image","http://wordpress.bloomberg.com/multimedia/files/2012/12/arrowBack.png");

        });

    

        function viewConnections(){

            $('div.contentContainer').hide();

            $('div#connectContainer').show();

            $(".mainNavLinks.active").removeClass("active");

            $('#connectNav').addClass("active");

        }

        function viewTrees(){

            if($('div#patriarchContainer').is(':visible')){

                $("#patriarchContainer div.bubble.elder[clan='" + $('#familyForm').val() + "']").click();

            } else {

                $('#familyForm').change();

                $('div.contentContainer').hide();

                $('div#familyTreeContainer').show();

                $(".mainNavLinks.active").removeClass("active");

                $('#familyTreeNav').addClass("active");

            }

        }

        function viewTrends(){

            $('div.contentContainer').hide();

            $('div#trendsContainer').show();

            $(".mainNavLinks.active").removeClass("active");

            $('#trendsNav').addClass("active");

        }

        function viewPatriarchs(){

            if($('div#familyTreeContainer').is(":visible")){

                var $myPat = $("#patriarchContainer div.bubble.elder[clan='" + $("#familyForm").val() +"']");

                $("#patriarchContainer div.bubble.elder:not([clan='" + $("#familyForm").val() +"'])").hide();

                $myPat.show();

                $myPat.css({top:530, left:400});

                var resetTop, resetLeft;

                if($myPat.parent().is("#patsRowOne")){

                    resetTop = 120;

                } else {

                    resetTop = 470;

                }

                if($myPat.is(":nth-child(1)")){

                    resetLeft = 0;

                } else if($myPat.is(":nth-child(2)")){

                    resetLeft = 245;

                } else if($myPat.is(":nth-child(3)")){

                    resetLeft = 490;

                } else {

                    resetLeft = 735;

                }

                $('div.contentContainer').hide();

                $('div#patriarchContainer').show();

                $myPat.animate({left:resetLeft}).animate({top:resetTop}, function(){

                    $("#patriarchContainer div.bubble.elder").css({top:"", left:""});

                    $("#patriarchContainer div.bubble.elder").not($myPat).fadeIn(200);

                    $("#patriarchContainer div.bubble.elder div.blurb").fadeIn(200);

                    $(".mainNavLinks.active").removeClass("active");

                    $('#patNav').addClass("active");

                });

            } else {

                $("#patriarchContainer div.bubble.elder").show();

                $("#patriarchContainer div.bubble.elder").removeAttr("style");

                $("#patriarchContainer div.bubble.elder div.blurb").removeAttr("style");

                $('div.contentContainer').hide();

                $('div#patriarchContainer').show();

                $(".mainNavLinks.active").removeClass("active");

                $('#patNav').addClass("active");

            }

        }

        

