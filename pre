        //jquery eventListener and Raphael hover event for family member nodes  

            $(familyMembers).each(function(){               

                var fullname = $(this.node).attr("fullname");

                var clanName = $(this.node).attr("clanName");

                var connectedPeople = [];

                var connectionText = connectedPeople.join("<br/>");

                this.hover(function(e){

                    this.attr({"cursor":"default"});

                    this.attr({"fill":colors[clanName],"stroke":"#ffffff"});

                    $(lines).each(function(){

                        var person1 = $(this.node).attr("person1");

                        var person2 = $(this.node).attr("person2");

                        if($(this.node).attr("person1") == fullname){ 

                            this.attr({"stroke-opacity":activeOpacity});

                          this.attr({"stroke":colors[clanName]});

                            connectedPeople.push(person2);

                        }

                        else if($(this.node).attr("person2") == fullname){

                            this.attr({"stroke-opacity":activeOpacity});

                          this.attr({"stroke":colors[clanName]});

                            connectedPeople.push(person1);

                        }

                    });

                    for(var i=0;i<familyMembers.length;i++){

                        if(this == familyMembers[i]){

                            showTooltip(e.pageX, e.pageY, "<b>"+tooltips["people"][i]+"</b> has links to<br/> "+connectedPeople.join(",<br/>"));

                        }

                    }

                },function(){

                    $(lines).each(function(){

                          this.attr({"stroke-opacity":inactiveOpacity});

                     });

                    this.attr({"fill":"#ffffff","stroke":colors[clanName]});

                    hideTooltip();

                    connectedPeople = [];

                });

            });

        

/*if person1 == fullname, highlight the line and set a connected person variable to person2. else if person2 == fullname, highlight the line and set a connected person variable to person1 */

        //jquery eventlistener and raphael hover event for individual lines             

        $(lines).each(function() {

            this.hover(function(e){

                for(var i=0;i<lines.length;i++){

                    if(this == lines[i]){

                        showTooltip(e.pageX, e.pageY, tooltips["connections"][i]);

                    }

                }

                this.attr({"stroke-opacity":activeOpacity});

                this.attr({"cursor":"default"});

                showTooltip(e.pageX, e.pageY, tooltips["connections"][eval(i)]);

            },function(){

                    this.attr({"stroke-opacity":inactiveOpacity});

                    hideTooltip();

            });

        });

        

        $(hoverLines).each(function() {

            this.hover(function(e){

                for(var i=0;i<hoverLines.length;i++){

                    if(this == hoverLines[i]){

                        showTooltip(e.pageX, e.pageY, tooltips["connections"][i]);

                        lines[i].attr({"stroke-opacity":activeOpacity});

                        lines[i].attr({"cursor":"default"});

                    }

                }

            },function(){

                for(var i=0;i<hoverLines.length;i++){

                    if(this == hoverLines[i]){

                        lines[i].attr({"stroke-opacity":inactiveOpacity});

                    }

                }

                hideTooltip();

            });

        });

    

        

        labels["wang"] =    canvas.text(610,90,"WANG")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor, "clanName":"wang"});

            (labels["wang"]).rotate(23,610,90);     

        labels["li"] =  canvas.text(790,280,"LI")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor});

            (labels["li"]).rotate(67,790,280);

        labels["peng"] =    canvas.text(780,540,"PENG")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor}); 

            (labels["peng"]).rotate(-65,780,540);   

        labels["deng"] =    canvas.text(590,715,"DENG")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor}); 

            (labels["deng"]).rotate(-18,590,715);

        labels["song"] =    canvas.text(340,705,"SONG")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor}); 

            (labels["song"]).rotate(20,340,705);

        labels["bo"] =  canvas.text(160,520,"BO")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor}); 

            (labels["bo"]).rotate(67,160,520);  

        labels["yang"] =    canvas.text(160,280,"YANG")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor}); 

            (labels["yang"]).rotate(-67,160,280);

        labels["chen"] =    canvas.text(320,95,"CHEN")

            .attr({"font-size":labelSize,"font-family":"arial,sans-serif","font-weight":"bold","fill":labelColor}); 

            (labels["chen"]).rotate(-23,320,95);

        

        

        //jquery eventListener and Raphael hover event for CLANS

        $.each(clans, function(clanName, path){ //here clan name is the mapped name and path is the stored object

            path.hover(function(e){

                this.attr({"cursor":"pointer"});

                $(lines).each(function(){

                    if($(this.node).attr("clan1") == clanName || $(this.node).attr("clan2") == clanName){

                       this.attr({"stroke-opacity":activeOpacity});

                       this.attr({"stroke":colors[clanName]});

                     }

                 });

                },function(){

                 $(lines).each(function(){

                      this.attr({"stroke-opacity":inactiveOpacity});

                        hideTooltip();

                 });

                }

             );

             path.click(function(){

                $("#familyForm").val(clanName);

                $('#familyTreeNav').click();

             });

             labels[clanName].hover(function(e){

                this.attr({"cursor":"pointer"});

                $(lines).each(function(){

                    if($(this.node).attr("clan1") == clanName || $(this.node).attr("clan2") == clanName){

                       this.attr({"stroke-opacity":activeOpacity});

                       this.attr({"stroke":colors[clanName]});

                     }

                 });

                },function(){

                 $(lines).each(function(){

                      this.attr({"stroke-opacity":inactiveOpacity});

                        hideTooltip();

                 });

                }

             );

             labels[clanName].click(function(){

                $("#familyForm").val(clanName);

                $('#familyTreeNav').click();

             });

        });

        clearInterval(loader);

        $('#connectNav').click();

});



function selectPatriarch(){

    $("#patriarchContainer div.bubble.elder").not(this).fadeOut(500);

    $("#patriarchContainer div.bubble.elder div.blurb").fadeOut(500);

    $(this).delay(500).animate({left:"400"}).animate({top:"530"}, function(){

        $("#familyForm").val($(this).attr("clan"));

        $('#familyForm').change();

        $('div.contentContainer').hide();

        $('div#familyTreeContainer').show();

        $(".mainNavLinks.active").removeClass("active");

        $('#familyTreeNav').addClass("active");

    });

}

function drawNode(cx,cy,clan,fullname,tooltip){

    familyMembers.push(canvas.circle(cx,cy,nodeRadius));

    tooltips["people"].push(tooltip);

    familyMembers[familyMembers.length-1].attr({stroke: colors[clan],fill:"#ffffff","stroke-width":2,"stroke-miterlimit":10});

    $(familyMembers[familyMembers.length-1].node).attr({"clanName":clan});

    $(familyMembers[familyMembers.length-1].node).attr({"fullname": fullname});

    

};

function drawLine(start,control1,control2,end,clan1,clan2,id,person1,person2,tooltip){

    lines.push(canvas.path([["M",start[0],start[1]],["C",control1[0],control1[1],control2[0],control2[1],end[0],end[1]]]));

    tooltips["connections"].push(tooltip);

    lines[lines.length - 1].attr({stroke: colors[clan1], "stroke-width":3,"stroke-linecap":"round"});

    lines[lines.length - 1].attr({"stroke-opacity":inactiveOpacity});

    $(lines[lines.length - 1].node).attr("clan1", clan1);

    $(lines[lines.length - 1].node).attr("clan2", clan2);

    $(lines[lines.length - 1].node).attr("person1", person1);

    $(lines[lines.length - 1].node).attr("person2", person2);

    $(lines[lines.length - 1].node).attr("tooltip", tooltip);

    /* NOW WE DRAW AN INVISIBLE LINE TWICE AS THICK TO ALSO SERVE AS HIT AREA */

    hoverLines.push(canvas.path([["M",start[0],start[1]],["C",control1[0],control1[1],control2[0],control2[1],end[0],end[1]]]));

    hoverLines[hoverLines.length - 1].attr({stroke: "#fff", "stroke-width":15,"stroke-linecap":"round"});

    hoverLines[hoverLines.length - 1].attr({"stroke-opacity":1});

    hoverLines[hoverLines.length - 1].toBack();

}

function showTooltip(x,y,tooltipText){

    $("div.tooltip").html(tooltipText);

    $("div.tooltip").addClass("mainText");

    $("div.tooltip").show();

}

function hideTooltip(){

    $(this).css("box-shadow", "");

    $("div.tooltip").hide();

}

    function toolTipper(e){

        $("div.tooltip").html($(this).attr("tooltip"));

        if($("div.tooltip").width() + e.pageX + 30 < $(window).width()){

            $("div.tooltip").css("left", e.pageX + 12);

        } else {

            $("div.tooltip").css("left", e.pageX - $("div.tooltip").width() - 20);

        }       

        $("div.tooltip").css("top", e.pageY + 15);

        $("div.tooltip").addClass("mainText");

        $(this).css("box-shadow", "2px 2px 2px #777");

        $("div.tooltip").html($(this).attr("tooltip"));

        $("div.tooltip").show();

    }

    function mouseMover(e){

        $("div.tooltip").html($("div.tooltip").html());

        if($("div.tooltip").width() + e.pageX + 30 < $(window).width()){

            $("div.tooltip").css("left", e.pageX + 12);

        } else {

            $("div.tooltip").css("left", e.pageX - $("div.tooltip").width() - 20);

        }

        $("div.tooltip").css("top", e.pageY + 15);

        $("div.tooltip").html($("div.tooltip").html());

    }

</script>

